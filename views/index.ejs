<%- include("./partials/header")%>

<% blogs.forEach(blog => { %>
    <div>
        <h1><%= blog.title%></h1>
        <img src="<%= blog.image%>" alt="">
        <span><%= blog.created%></span>
        <p><%= blog.body%></p>
    </div>
<% })%>

